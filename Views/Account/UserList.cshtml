@model IEnumerable<Microsoft.AspNetCore.Identity.IdentityUser>
@{
    Layout = "_LayoutAdmin";
}

<h2>User list</h2>
<a asp-controller="Account" asp-action="CreateUser" class="btn btn-info mb-3">Create user</a>
<table class="table table-bordered">
    <tr>
        <th>Username</th>
        <th>Email</th>
    </tr>
    @foreach (var user in Model) {
        <tr>
            <td>@user.UserName</td>
            <td>@user.Email</td>
        </tr>
    }
</table>