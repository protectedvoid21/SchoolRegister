@model SchoolRegister.Models.Message

<p style="font-size: 1.5em">From : @Model.SenderUser.Name @Model.SenderUser.Surname</p>
<div class="card">
    <div class="card-header d-flex">
        <h5 class="m-auto">@Model.Title</h5>
        <h5>@Model.CreatedDate</h5>
    </div>
    <div class="card-body">
        @Model.Description
    </div>
</div>